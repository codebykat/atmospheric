{% extends "layout.html.j2" %}

{% block stylesheets %}
	<link href="css/style.css" rel="stylesheet">
	<link href="css/home.css" rel="stylesheet">
{% endblock %}

{% block content %}

	{% for cloud in clouds %}
		<div class="tile" style="background-image:url({{ cloud.thumbnail}})" >
			<a href="{{ cloud.url }}">
				<!-- <img src="{{ cloud.thumbnail }}" height="50" width="50" /> -->
			</a>
		</div>
	{% endfor %}

{% endblock %}
